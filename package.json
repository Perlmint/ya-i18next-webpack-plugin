{
  "name": "ya-i18next-webpack-plugin",
  "version": "0.4.0",
  "description": "",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "build": "tsc -p src",
    "test": "cross-env TS_NODE_PROJECT=test/tsconfig.json TS_NODE_FILES=true nyc mocha --require ts-node/register \"test/*.ts\"",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "prepare": "npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Perlmint/ya-i18next-webpack-plugin.git"
  },
  "keywords": [
    "i18next",
    "webpack",
    "plugin",
    "i18n"
  ],
  "author": "Gyusun Yeom <omniavinco@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Perlmint/ya-i18next-webpack-plugin/issues"
  },
  "homepage": "https://github.com/Perlmint/ya-i18next-webpack-plugin#readme",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/i18next": "^8.4.6",
    "@types/i18next-node-fs-backend": "0.0.30",
    "@types/lodash": "^4.14.134",
    "@types/mocha": "^2.2.48",
    "@types/node": "^8.10.49",
    "@types/stream-buffers": "^3.0.3",
    "@types/tmp": "0.0.33",
    "@types/webpack": "^3.8.18",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "tmp": "0.0.33",
    "ts-node": "^8.2.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "i18next": "^9.1.0",
    "i18next-node-fs-backend": "^2.1.3",
    "lodash": "^4.17.11",
    "source-map": "^0.6.1",
    "stream-buffers": "^3.0.2",
    "virtual-module-webpack-plugin": "^0.4.1",
    "webpack": "^3.12.0"
  },
  "files": [
    "config.d.ts",
    "index.js",
    "index.d.ts"
  ],
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "test",
      "coverage",
      "*.js"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "all": true
  }
}
